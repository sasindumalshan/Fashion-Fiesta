<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="541.0" prefWidth="535.0" stylesheets="@../style/EmployeeDashBord.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.fashionfiesta.controller.SupplierUpdateFormController">
   <children>
      <Label layoutX="201.0" layoutY="14.0" prefHeight="56.0" prefWidth="148.0" text="Update">
         <font>
            <Font name="System Bold" size="39.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtFirstName" alignment="CENTER" layoutX="14.0" layoutY="123.0" onKeyReleased="#firstNameKeyReleased" prefHeight="31.0" prefWidth="200.0" promptText="First name" />
      <JFXTextField fx:id="txtLastName" alignment="CENTER" layoutX="321.0" layoutY="123.0" onKeyReleased="#lastNameKeyReleased" prefHeight="31.0" prefWidth="200.0" promptText="Last name" />
      <JFXTextField fx:id="txtStreet" alignment="CENTER" layoutX="14.0" layoutY="202.0" onKeyReleased="#streetKeyReleased" prefHeight="31.0" prefWidth="200.0" promptText="street" />
      <JFXTextField fx:id="txtCity" alignment="CENTER" layoutX="321.0" layoutY="202.0" onKeyReleased="#cityKeyReleased" prefHeight="31.0" prefWidth="200.0" promptText="city" />
      <JFXButton fx:id="btnUpdate" layoutX="177.0" layoutY="449.0" onAction="#btnUpdateOnAction" prefHeight="42.0" prefWidth="182.0" style="-fx-background-color: black; -fx-background-radius: 20;" text="Update" textFill="WHITE" />
      <JFXTextField fx:id="txtLane" alignment="CENTER" layoutX="14.0" layoutY="287.0" onKeyReleased="#laneKeyReleased" prefHeight="31.0" prefWidth="200.0" promptText="Lane" />
      <JFXTextField fx:id="txtContact" alignment="CENTER" layoutX="321.0" layoutY="287.0" prefHeight="31.0" prefWidth="200.0" promptText="Contact number" />
   </children>
</AnchorPane>
